#include<stdio.h>
main()

{
	float a,b,c,d,x;
	printf("enter items sold");
	scanf("%f",&a);
	printf("enter value");
	scanf("%f",&b);
    printf("enter discount");
	scanf("%f",&c);
    printf("enter tax");
	scanf("%f",&d);
	x=a*b;
	c=x*(c/100);
	x=x-c;
	d=x*(d/100);
	x=x+d;
	printf("billing amount is %f",x);
}
